<launch>
  <arg name="port" default="/dev/ttyUSB-pl2303" />
  <arg name="baud" default="2000000" />
  <arg name="dummy_port" default="/tmp/ttyDUMMY" />
  <node pkg="arm_robot_msgs" type="wrap-interceptty.bash" name="interceptty" args="$(arg port) $(arg baud) $(arg dummy_port)" output="screen" /> 
  <node pkg="rosserial_server" type="serial_node" name="rosserial_server" output="screen" >
    <param name="port" value=" $(arg dummy_port)" />
    <param name="baud" value="$(arg baud)" />
  </node>
  <node pkg="rosserial_python" type="message_info_service.py" name="rosserial_message_info" />
  <node pkg="arm_robot_msgs" type="msg_bridge" name="msg_bridge" output="screen" />
</launch>
